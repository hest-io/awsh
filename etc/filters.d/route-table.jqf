#cmd: ec2 describe-route-tables
#sort: VpcId,Name,RouteTableId

.RouteTables[] | {
    VpcId,
    Name: ((.Tags[]?|select(.Key=="Name")|.Value) // "None"),
    RouteTableId,
    PropagatingVGWs: (.PropagatingVgws|join(", ")),
    Associations: [(.Associations|.[]| (.SubnetId?) // "Main")] | join(", ")
}